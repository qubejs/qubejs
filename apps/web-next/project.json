{
  "name": "web-next",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/web-next",
  "projectType": "application",
  "tags": [],
  "targets": {
    "typegen": {
      "executor": "sanity schema extract --path=apps/web-next/src/sanity/extract.json && sanity typegen generate"
